class Settings():
    def _init_(self):
        self.screen_width = 1200
        self.screen_height = 800
        self.bg_color = (230, 230, 230)
class Settings():
    def __init__(self):
        self.ship_speed_factor = 1.5
    def __init__(self):
        self.bullet_speed_factor = 1
        self.bullet_width = 3
        self.bullet_height = 15
        self.bullet_color = 60, 60, 60